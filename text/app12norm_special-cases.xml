    <!--       10909[von Festen]{=} <i>10909</i> von Festen <i>2_H</i>] von Fasten <i>2_IV_H.20 :</i> vom Fasten <i>G</i> von Festen <i>2_H mglw aus</i> von Fasten <i>[type=2d]</i>-->
    <app n="10909">
        <f:replace n="10909">von Festen</f:replace>
        <f:ins n="10909">von Festen</f:ins>
        <ref>10909</ref>
        <!--von Festen <i>2_H</i>-->
        <lem wit="2_H">von Festen <note><wit wit="2_H">2_H</wit></note></lem>
        <!-- von Fasten <i>2_IV_H.20 :</i>-->
        <rdg wit="2_IV_H.20">von Fasten <note><wit wit="2_IV_H.20">2_IV_H.20</wit></note></rdg>
        <!-- vom Fasten <i>G</i>-->
        <!-- aus py: -->
        <!--<rdg wit="2_IV_H.20" hand="G">vom Fasten <note><seg type="hand">G</seg></note></rdg>
        <!-\- von Festen <i>2_H mglw aus</i>-\->
        <rdg wit="2_H">von Festen <note><wit wit="2_H">2_H</wit> mglw aus</note></rdg>
        <!-\- von Fasten <i>[type=2d]</i>-\->
        <rdg type="type_2d">von Fasten</rdg>-->
        <!-- neu: -->
        <rdg wit="2_IV_H.20" hand="G">vom Fasten <note><seg type="hand">G</seg></note></rdg>
        <rdg wit="2_H" type="type_2d">von Festen <note>mglw aus <quote>von Fasten</quote>
                <wit wit="2_H">2_H</wit></note></rdg>
    </app>
