<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="appxnorm.rnc" type="application/relax-ng-compact-syntax"?>
<f:apparatus xmlns:f="http://www.faustedition.net/ns" xmlns="http://www.tei-c.org/ns/1.0">
    <!--       before_5357_b[]{} <i>vor 5357</i> <speaker>Alecto</speaker> <i>2_I_H.0a</i>] <font name="Cambria Math">⟨</font><stage>Die Furien</stage> <i>erg G</i><font name="Cambria Math">⟩</font> <speaker>Alecto.</speaker> <i>2_H [type=2c*]</i>-->
    <!--FIXED: before_5357_b[]{} <i>vor 5357</i> <speaker>Alecto</speaker> <i>2_I_H.0a</i>] ⟨<stage>Die Furien</stage> <i>erg G</i>⟩ <speaker>Alecto.</speaker> <i>2_H [type=2c*]</i>-->
    <app n="before_5357_b">
        <f:ins n="before_5357_b" place="only-app"/>
        <ref>vor 5357</ref>
        <!--<speaker>Alecto</speaker> <i>2_I_H.0a</i>-->
        <lem wit="2_I_H.0a">
            <speaker>Alecto</speaker>
            <note>
                <wit wit="2_I_H.0a">2_I_H.0a</wit>
            </note>
        </lem>
        <!-- aus py: -->
        <!--<!-\- ⟨<stage>Die Furien</stage> <i>erg G</i>-\->
        <rdg hand="G">⟨<stage>Die Furien</stage>
            <note>erg <seg type="hand">G</seg></note></rdg>
        <!-\-⟩ <speaker>Alecto.</speaker> <i>2_H [type=2c*]</i>-\->
        <rdg wit="2_H" type="type_2c*">⟩ <speaker>Alecto.</speaker>
            <note><wit wit="2_H">2_H</wit></note></rdg>-->
        <!-- neu: -->
        <rdg wit="2_H" type="type_2c*">⟨<stage>Die Furien</stage>
            <note>erg <seg type="hand">G</seg></note>⟩ <speaker>Alecto.</speaker>
            <note><wit wit="2_H">2_H</wit></note></rdg>
    </app>
    <!--       before_6086[Marschalck]{=} <i>vor 6086</i> <speaker>Marschalck</speaker> <i>2_H</i>] <speaker>Schatzmeister</speaker> <i>2_H :</i> <speaker>Marschalck</speaker> <i>G (</i><speaker>Schatzmeister</speaker> <i>ungestrichen) [type=7]</i>-->
    <app n="before_6086">
        <f:replace n="before_6086">Marschalck</f:replace>
        <f:ins n="before_6086">Marschalck</f:ins>
        <ref>vor 6086</ref>
        <!--<speaker>Marschalck</speaker> <i>2_H</i>-->
        <lem wit="2_H"><speaker>Marschalck</speaker>
            <note><wit wit="2_H">2_H</wit></note></lem>
        <!-- aus py: -->
        <!--<!-\- <speaker>Schatzmeister</speaker> <i>2_H :</i>-\->
        <rdg wit="2_H"><speaker>Schatzmeister</speaker>
            <note><wit wit="2_H">2_H</wit></note></rdg>
        <!-\- <speaker>Marschalck</speaker> <i>G (</i>-\->
        <rdg wit="2_H" hand="G"><speaker>Marschalck</speaker>
            <note><seg type="hand">G</seg> (</note></rdg>
        <!-\-<speaker>Schatzmeister</speaker> <i>ungestrichen) [type=7]</i>-\->
        <rdg type="type_7"><speaker>Schatzmeister</speaker>
            <note>ungestrichen)</note></rdg>-->
        <!-- neu: -->
        <rdg wit="2_H"><subst hand="G"><del><speaker>Schatzmeister</speaker></del>
                <note><wit wit="2_H">2_H</wit></note>
                <add><speaker>Marschalck</speaker></add></subst>
            <note><seg type="hand">G</seg> (<quote>Schatzmeister</quote> ungestrichen)</note></rdg>
    </app>
    <!--       before_6319_a[]{} <i>vor 6319</i> <speaker>Blondine</speaker> <i>2_H</i>] <speaker>Blondine</speaker> <font name="Cambria Math">⟨</font><stage>(zu Mephisto)</stage> <i>erg Ec</i><font name="Cambria Math">⟩</font> <i>2_H [type=6]</i>-->
    <!--FIXED: before_6319_a[]{} <i>vor 6319</i> <speaker>Blondine</speaker> <i>2_H</i>] <speaker>Blondine</speaker> ⟨<stage>(zu Mephisto)</stage> <i>erg Ec</i>⟩ <i>2_H [type=6]</i>-->
    <app n="before_6319_a">
        <f:ins n="before_6319_a" place="only-app"/>
        <ref>vor 6319</ref>
        <!--<speaker>Blondine</speaker> <i>2_H</i>-->
        <lem wit="2_H"><speaker>Blondine</speaker> <note><wit wit="2_H">2_H</wit></note></lem>
        <!-- aus py: -->
        <!--<!-\- <speaker>Blondine</speaker> ⟨<stage>(zu Mephisto)</stage> <i>erg Ec</i>-\->
        <rdg><speaker>Blondine</speaker> ⟨<stage>(zu Mephisto)</stage> <note>erg Ec</note></rdg>
        <!-\-⟩ <i>2_H [type=6]</i>-\->
        <rdg wit="2_H" type="type_6">⟩ <note><wit wit="2_H">2_H</wit></note></rdg>-->
        <!-- neu: -->
        <rdg wit="2_H" type="type_6"><speaker>Blondine</speaker> ⟨<stage>(zu Mephisto)</stage> <note>erg Ec</note>⟩ <note><wit wit="2_H">2_H</wit></note></rdg>    </app>
</f:apparatus>
